<feed xmlns='http://www.w3.org/2005/Atom' xmlns:apps='http://schemas.google.com/apps/2006'>
  <title>YouTuber and Developer Gmail Filters v1.0.0</title>
  <updated>2025-09-07T00:00:00Z</updated>
  <author><name>YouTuber-Developer</name></author>

  <!-- YOUTUBE: Creator Hub notifications for monetization and channel management (important) -->
  <entry>
    <category term='filter'/>
    <title>YouTube - Creator Hub</title>
    <updated>2025-09-07T00:00:00Z</updated>
    <content/>
    <apps:property name='from' value='noreply@youtube.com'/>
    <apps:property name='hasTheWord' value='subject:(Creator OR monetization OR analytics OR &quot;Community Guidelines&quot; OR copyright OR channel)'/>
    <apps:property name='label' value='YouTube/Creator Hub'/>
    <apps:property name='shouldAlwaysMarkImportant' value='true'/>
  </entry>

  <!-- YOUTUBE: Community engagement notifications for comments and subscribers (important) -->
  <entry>
    <category term='filter'/>
    <title>YouTube - Community</title>
    <updated>2025-09-07T00:00:00Z</updated>
    <content/>
    <apps:property name='from' value='noreply@youtube.com'/>
    <apps:property name='hasTheWord' value='subject:(commented OR replied OR &quot;liked your&quot; OR subscribed OR &quot;new subscriber&quot;)'/>
    <apps:property name='label' value='YouTube/Community'/>
    <apps:property name='shouldAlwaysMarkImportant' value='true'/>
  </entry>

