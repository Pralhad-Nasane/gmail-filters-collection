  <!-- NEWSLETTERS: Technology and development newsletters (auto-archived) -->
  <entry>
    <category term='filter'/>
    <title>Newsletters - Tech</title>
    <updated>2025-09-07T00:00:00Z</updated>
    <content/>
    <apps:property name='hasTheWord' value='(has:unsubscribe OR list:) subject:(dev OR code OR programming OR AI OR tech OR software)'/>
    <apps:property name='label' value='Newsletters/Tech'/>
    <apps:property name='shouldArchive' value='true'/>
  </entry>

  <!-- NEWSLETTERS: General non-technical newsletters and publications (auto-archived) -->
